<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pcwk.mypage">

    <select id="getMemberById" parameterType="string" resultType="com.pcwk.ehr.cmn.MyPageVO">
        SELECT * FROM your_table_name WHERE userId = #{userId}
    </select>
    
    
    <update id="updateUser" parameterType="com.pcwk.ehr.cmn.MyPageVO">
        UPDATE your_table_name
        SET name = #{name},
            email = #{email},
            birthday = #{birthday},
            gender = #{gender},
            keyword = #{keyword}
        WHERE userNo = #{userNo}
    </update>



</mapper>